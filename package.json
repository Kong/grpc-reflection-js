{
  "name": "grpc-reflection-js",
  "version": "0.0.1",
  "main": "build/src/index.js",
  "repository": "https://github.com/redhoyasa/grpc-reflection-js",
  "author": "redhoyasa@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.0.3",
    "@grpc/proto-loader": "^0.5.4",
    "@types/async": "^3.2.3",
    "@types/google-protobuf": "^3.7.2",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/protobufjs": "^6.0.0",
    "async": "^3.2.0",
    "grpc": "^1.24.2",
    "grpc-tools": "^1.9.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "protobufjs": "^6.9.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.1",
    "@types/sinon": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "chai": "^4.2.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "grpc_tools_node_protoc_ts": "^4.0.0",
    "gts": "^2.0.2",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "protoc-gen-grpc": "^1.4.0",
    "sinon": "^9.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "test": "nyc mocha -r ts-node/register test/**/*.test.ts",
    "coverage": "nyc report --reporter=json > coverage/coverage.json",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile",
    "posttest": "yarn run check",
    "protoc-pb": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --js_out=import_style=commonjs,binary:./src --ts_out=./src -I ./static/grpc/reflection/v1alpha reflection.proto",
    "protoc-grpc": "protoc-gen-grpc --js_out=import_style=commonjs,binary:./src --grpc_out=./src -I ./static/grpc/reflection/v1alpha reflection.proto",
    "protoc": "yarn run protoc-pb && yarn run protoc-grpc"
  },
  "files": [
    "src",
    "build",
    "LICENSE"
  ],
  "keywords": [
    "grpc"
  ]
}
